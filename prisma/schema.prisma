datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            String @id @default(cuid())
  externalId    String @unique
  email         String
  displayName   String
  profileImgUrl String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
